/* 头部 */
page{
  padding-bottom: 30rpx;
}


/* 头部导航 */
.head-nav {
  padding: 5px 0;
  background: rgba(0,0,0,0.1);
  margin-top: 30rpx;
}
.head-nav-item {
  width: 25%;
  padding: 5rpx 0;
}
.head-nav-item view {
  line-height: 38rpx;
}
.head-nav-item view, .message-nav text {
  color: #ffd7d7;
}
.message-nav {
  position: absolute;
  top: 20rpx;
  right: 20rpx;
}
.message-nav image {
  width: 28rpx;
  height: 28rpx;
  vertical-align: middle;
}
.message-nav text {
  margin-left: 5rpx;
  font-size: 24rpx;
}
.bg-ph{
  margin: 0;
  background: #d2364c;
}
/* 导航列表 */
.nav-box .nav-item {
    height: 100rpx;
    box-sizing: border-box;
    padding: 30rpx 20rpx;
}
.nav-box .nav-item .item-icon {
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
}
.nav-box .nav-item .item-name {
    margin-left: 20rpx;
    vertical-align: middle;
}
.nav-box .nav-item .item-desc {
  margin-right: 50rpx;
  margin-top: 2rpx;
  width: calc(100% - 300rpx);
}
.nav-box .nav-item .item-arrow {
    width: 25rpx;
}

.items-list {
  padding: 15rpx 0;
}
.items-list .items {
  width: 25%;
  position: relative;
}
.items-list .items .items-icon {
  width: 60rpx;
  height: 60rpx;
  margin-bottom: 6rpx;
}
.items-list .items .badge-icon {
  position: absolute;
  top: -2px;
  left: calc(50% + 35rpx);
}

/* 通知 */
.user-notice { margin-top: 20rpx; padding: 0 10rpx 20rpx 10rpx; }

.share-box{ 
  /* background-color: red;  */
   background-image: linear-gradient(30deg,rgb(56, 155, 248),rgba(17, 103, 184, 0.305)),linear-gradient(-30deg,rgb(6, 114, 216),rgb(88, 165, 236));
  width: 70%; margin: auto;
  height: 250rpx;
  border-radius:20rpx;
  margin-top: 50rpx; 
  padding: 50rpx; 
  color: #FFF
}
.share-box .share-box-avatar{ width: 30rpx; border-radius: 50%;}
.share-box .progress{ width: 100%; height: 20rpx; background-color: #FFF; margin: 20rpx 0rpx;}
.share-box .share-btn{ width: 100%; margin: 40rpx 0rpx;}

.share-pic image{ width: 100%;}


/* ph添加 */
.how-money-top{
  width: 100%;
}
.how-money-top .how-top{
  position: relative;
  width: 100%;
}
.plan-view{
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 240rpx;
  width: 70%;
  height: 30rpx;
  
  /* background: #fff; */
}
.control-view{
  border-radius: 40rpx;
  overflow: hidden;
}
.hint{
  position: absolute;
  top:-90rpx;
  width: 130rpx;
  line-height: 60rpx;
  background: #F6D4A8;
  border-radius: 25rpx;
  text-align: center;
  font-size: 28rpx;
  color:#d43c33;
}
.hint::after{
  content: '';
  display: block;
  position: absolute;
  left:30%;
  bottom: -20rpx;
  width: 0;
  height: 0;
  border-top:20rpx solid #F6D4A8;
  border-right: 20rpx solid transparent;
}
.movable-area{
  position: absolute;
  top:0;
  left:0;
  width: 100%;
}
.movable-view{
  width: 20rpx;
  height: 30rpx;
  background: #F6D4A8;
  opacity: 0;
}
.tbotton{
  position: absolute;
  bottom: 92rpx;
  left:50%;
  transform: translateX(-50%);
}

.invite-botton{
  position: absolute;
  bottom: 92rpx;
  width: 80%;
  height: 110rpx;
  /* border:1px red solid; */
  left:50%;
  transform: translateX(-50%);
}
.how-money-top .how-top image{
  width: 100%;
  height: 690rpx;
}
.how-money-top .top-two{
  width: 100%;
  transform: translateY(-10rpx);
}
.top-two image{
  width: 100%;
  height: 400rpx;
}
